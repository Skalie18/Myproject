  <DataSources>
    <DataSource Name="dsFilesSubmitions">
      <DataSourceReference>dsFDRReport</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>2a92b7fa-08c2-4e46-9980-96977603fbdf</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="dsIRQ08">
      <Query>
        <DataSourceName>dsFilesSubmitions</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@FileCategoryId">
            <Value>=Parameters!FileCategoryId.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@TaxRefNo">
            <Value>=Parameters!TaxRefNo.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT DISTINCT a.Taxrefno, a.AssessmentYear AS [Tax Year], d.Description AS [File Type], c.Description AS Status, COUNT(1) AS [No Of Files]
FROM     Files AS a INNER JOIN
                  FileSubmissions AS b ON a.SubmissionId = b.SubmissionId INNER JOIN
                  FileSubmissionStatuses AS c ON c.Id = b.SubmissionStatusId INNER JOIN
                  FileCategories AS d ON a.FileCategoryID = d.ID
WHERE  (a.FileCategoryID = @FileCategoryId) AND (CAST(b.Timestamp AS date) BETWEEN @StartDate AND @EndDate) AND (a.Taxrefno = @TaxRefNo)
GROUP BY b.SubmissionId, c.Description, a.Taxrefno, a.AssessmentYear, d.Description</CommandText>
      </Query>
      <Fields>
        <Field Name="Taxrefno">
          <DataField>Taxrefno</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Tax_Year">
          <DataField>Tax Year</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="File_Type">
          <DataField>File Type</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Status">
          <DataField>Status</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="No_Of_Files">
          <DataField>No Of Files</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="dsFileCategories">
      <SharedDataSet>
        <SharedDataSetReference>dsFileCategories</SharedDataSetReference>
      </SharedDataSet>
      <Fields>
        <Field Name="ID">
